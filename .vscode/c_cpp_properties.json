{
  "version": 4,
  "env": {
    "projectPaths": ["${workspaceFolder}/**"]
  },
  "configurations": [
    {
      "name": "Linux",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "cppStandard": "c++11",
      "defines": [],
      "includePath": [
        "${projectPaths}",
        "/usr/include/**",
        "/usr/lib/x86_64-linux-gnu/**"
      ],
      "browse": {
        "path": [
          "${projectPaths}",
          "/usr/include/**",
          "/usr/lib/x86_64-linux-gnu/**"
        ]
      }
    },
    {
      "name": "Mac",
      "intelliSenseMode": "macos-gcc-arm64",
      "compilerPath": "/opt/homebrew/bin/gcc-11",
      "cStandard": "c11",
      "cppStandard": "c++11",
      "defines": [],
      "macFrameworkPath": [],
      "includePath": [
        "${projectPaths}",
        "/opt/homebrew/Cellar/gtk4/4.6.1/include/gtk-4.0/**",
        "/opt/homebrew/Cellar/glib/2.72.0/include/glib-2.0/**",
        "/opt/homebrew/Cellar/glib/2.72.0/lib/glib-2.0/include/**",
        "/opt/homebrew/Cellar/cairo/1.16.0_5/include/cairo/**",
        "/opt/homebrew/Cellar/pango/1.50.6/include/pango-1.0/**",
        "/opt/homebrew/Cellar/harfbuzz/4.1.0/include/harfbuzz/**",
        "/opt/homebrew/Cellar/gdk-pixbuf/2.42.8/include/gdk-pixbuf-2.0/**",
        "/opt/homebrew/Cellar/graphene/1.10.8/include/graphene-1.0/**",
        "/opt/homebrew/Cellar/graphene/1.10.8/lib/graphene-1.0/include/**"
      ],
      "browse": {
        "path": ["${projectPaths}", "/opt/homebrew/Cellar/**"]
      }
    }
  ]
}
